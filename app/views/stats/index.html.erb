<p>
  <%= @last_commit_message %>
</p>
<p>
  <b>Chats count:</b> <%= @chats_count %>
  <b>Matches count:</b> <%= @matches_count %>
  <b>Leagues count:</b> <%= @leagues_count %>
</p>
<hr/>
<p><b>Live matches:</b></p>
<% @live_matches.each do |match| %>
  <%= "[#{match.league_name} | #{match.tier} | #{match.id}] #{match.radiant} VS #{match.dire}" %>
  </br>
<% end %>
<hr/>
<p><b>Recent matches:</b></p>
<% @finished_matches.each do |match| %>
  <%= "[#{match.league_name} | #{match.tier} | #{match.id}] #{match.radiant} VS #{match.dire}" %>
  </br>
<% end %>
<hr/>
